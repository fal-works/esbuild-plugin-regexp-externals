{
  "name": "@fal-works/esbuild-plugin-regexp-externals",
  "description": "esbuild plugin for specifying external modules with regular expressions.",
  "version": "0.1.0",
  "license": "CC0-1.0",
  "author": "FAL",
  "keywords": [
    "esbuild"
  ],
  "homepage": "https://github.com/fal-works/esbuild-plugin-regexp-externals#readme",
  "bugs": {
    "url": "https://github.com/fal-works/esbuild-plugin-regexp-externals/issues"
  },
  "type": "module",
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "module": "lib/index.js",
  "exports": "./lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "esbuild": "^0.8.39"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "tsc && npm run format:lib",
    "preformat:lib": "eslint --fix --no-ignore lib/**/*.{js,ts}",
    "format:lib": "prettier --write lib/**/*.{js,ts}",
    "lint": "eslint src/"
  }
}
